{
  "name": "dekodeinteraktiv/hogan-content-grid",
  "description": "Content Grid Module for Hogan.",
  "homepage": "https://github.com/dekodeinteraktiv/hogan-content-grid",
  "type": "wordpress-plugin",
  "license": "GPL-3.0-or-later",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=7.0",
    "composer/installers": "~1.2",
    "dekodeinteraktiv/hogan-core": ">=1.1.7"
  },
  "archive": {
    "exclude": [
      ".editorconfig",
      ".gitignore",
      "composer.json",
      "composer.lock",
      "LICENSE",
      "phpcs.xml"
    ]
  },
  "require-dev": {
    "dekodeinteraktiv/coding-standards": "^0.3.1"
  }
}
